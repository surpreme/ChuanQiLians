<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.alllocalagent.localagent.LocalAgentActivity">

    <com.mbcq.baselibrary.view.CnToolbar
        android:id="@+id/local_agent_toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="@color/base_blue"
        app:backButtonIsShow="true"
        app:centerTextViewText="中转记录"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.tabs.TabLayout
        android:id="@+id/local_agent_tabLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/local_agent_toolbar" />

    <fragment
        android:id="@+id/localagent"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:name="com.mbcq.vehicleslibrary.fragment.localagentshortfeeder.LocalGentShortFeederFragment"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/local_agent_tabLayout" />
</androidx.constraintlayout.widget.ConstraintLayout>